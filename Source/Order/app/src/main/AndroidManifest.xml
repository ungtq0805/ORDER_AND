        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">

            <!-- Main Activity -->

        </activity>
        <activity android:name=".activity.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
